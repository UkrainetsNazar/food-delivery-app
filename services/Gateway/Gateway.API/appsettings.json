{
  "ReverseProxy": {
    "Routes": {
      "auth_route": {
        "ClusterId": "auth_cluster",
        "Match": {
          "Path": "/auth/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth_cluster": {
        "Destinations": {
          "auth_service": {
            "Address": "https://localhost:7192/"
          }
        }
      }
    }
  }
}