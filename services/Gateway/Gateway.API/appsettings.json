{
  "ReverseProxy": {
    "Routes": {
      "auth_route": {
        "ClusterId": "auth_cluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      },
      "admin_panel_route": {
        "ClusterId": "admin_panel_cluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      },
      "restaurant_route": {
        "ClusterId": "restaurant_cluster",
        "Match": {
          "Path": "/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth_cluster": {
        "Destinations": {
          "auth_service": {
            "Address": "https://localhost:7192/"
          }
        }
      },
      "admin_panel_cluster": {
        "Destinations": {
          "admin_panel_service": {
            "Address": "https://localhost:7146/"
          }
        }
      },
      "restaurant_cluster": {
        "Destinations": {
          "restaurant_service": {
            "Address": "https://localhost:7155/"
          }
        }
      }
    }
  }
}
